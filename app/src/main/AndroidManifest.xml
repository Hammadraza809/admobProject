<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.project">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".SavingDataActivity"
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MenuActivity"></activity>
        <activity android:name=".CalculateActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FreeCalculatorScreenActivity" />
        </activity>
        <activity android:name=".ReferAFriendActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FreePointsScreenActivity" />
        </activity>
        <activity android:name=".CompleteOffersActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FreePointsScreenActivity" />
        </activity>
        <activity android:name=".InstallAppActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FreePointsScreenActivity" />
        </activity>
        <activity android:name=".RateAppActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FreePointsScreenActivity" />
        </activity>
        <activity android:name=".WatchVideosActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FreePointsScreenActivity" />
        </activity>
        <activity android:name=".FreeSpinActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FreePointsScreenActivity" />
        </activity>
        <activity android:name=".FreePointsScreenActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeScreenActivity" />
        </activity>
        <activity android:name=".FreeCoinsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FreeCalculatorScreenActivity" />
        </activity>
        <activity android:name=".FreeGemsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FreeCalculatorScreenActivity" />
        </activity>
        <activity android:name=".FreeCalculatorScreenActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeScreenActivity" />
        </activity>
        <activity android:name=".UseScreenActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeScreenActivity" />
        </activity>
        <activity android:name=".HomeScreenActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".StartScreenActivity" />
        </activity>
        <activity android:name=".StartScreenActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.AD_MANAGER_APP"
            android:value="true" />
    </application>

</manifest>